{{ form_start(form) }}
<div class="row">
<div class="col-10 col-lg-5 mb-1">
	{{ form_row(form.email, {attr: {class: 'form-label form-control'}}) }}
</div>

<div class="col-10 col-lg-5 mb-1">
	{{ form_row(form.password, {attr: {class: 'form-label form-control'}}) }}
</div>

{# <div class="col-10 col-lg-5 mb-1">
</div> #}

<div class="col-10 col-lg-5 mb-1">
	{{ form_row(form.firstName, {attr: {class: 'form-label form-control'}}) }}
</div>

<div class="col-10 col-lg-5 mb-1">
	{{ form_row(form.lastName, {attr: {class: 'form-label form-control'}}) }}
</div>
<div class="col-10 mb-1">
	{{ form_row(form.address, {attr: {class: 'form-label form-control'}}) }}
</div>
<div class="col-10 col-lg-5 mb-1">
	{{ form_row(form.zipCode, {attr: {class: 'form-label form-control'}}) }}
</div>
<div class="col-10 col-lg-5 mb-1">
	{{ form_row(form.city, {attr: {class: 'form-label form-control'}}) }}
</div>
<div class="col-10 mb-1">
	{{ form_row(form.image, {attr: {class: 'form-label form-control'}}) }}
</div>

<div class="d-flex align-items-center w-100 flex-column">
<button class="btn btn-success">{{ button_label|default('Enregistrer') }}</button>

</div>
</div>
{{ form_end(form) }}
